{
  "id":   "/resume",
  "cmd":  null,
  "cpus": 0.1,
  "mem":  128,
  "disk": 0,
  "instances":  1,
  "container": {
    "type":     "DOCKER",
    "volumes":  [],
    "docker": {
      "image":    "horgix/resume",
      "network":  "BRIDGE",
      "portMappings": [
        {
          "containerPort":  80,
          "hostPort":       0,
          "servicePort":    10007,
          "protocol":       "tcp",
          "name":           "80",
          "labels":         {}
        }
      ],
      "parameters":     [],
      "privileged":     false,
      "forcePullImage": false
    }
  },
  "healthChecks": [
    {
      "path": "/",
      "protocol":   "HTTP",
      "portIndex":  0,
      "gracePeriodSeconds": 300,
      "intervalSeconds":    60,
      "timeoutSeconds":     20,
      "maxConsecutiveFailures": 3,
      "ignoreHttp1xx":      false
    }
  ],
  "labels": {
    "traefik.enable":   "true",
    "traefik.backend":  "resume",
    "traefik.frontend.rule": "Host:cv.horgix.eu,resume.horgix.eu"
  },
  "portDefinitions": [
    {
      "port":     10007,
      "protocol": "tcp",
      "labels":   {}
    }
  ]
}
